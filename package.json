{
  "name": "@pawover/types",
  "author": "pawover<pawover@outlook.com>",
  "description": "pawover's types",
  "license": "MIT",
  "type": "module",
  "version": "0.0.0-alpha.7",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=22.20.0"
  },
  "keywords": [
    "pawover",
    "pawover-types"
  ],
  "homepage": "https://github.com/pawover/pawover-types#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/pawover/pawover-types.git"
  },
  "main": "./dist/index.d.ts",
  "module": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "public": "pnpm build && npm publish --access public",
    "check": "pnpm check:types & pnpm check:eslint & pnpm check:format",
    "check:types": "tsc --noEmit",
    "check:eslint": "eslint --fix \"**/*.{js,cjs,mjs,ts,cts,mts,jsx,tsx,vue}\" --cache-location=eslint.cache.json --cache",
    "check:format": "prettier --write \"**/*.{html,json}\" !.vscode/settings.json --cache-location=prettier.cache.json --cache",
    "check:pack": "attw --pack .",
    "clean": "pnpm clean:cache & pnpm clean:lib & pnpm clean:output",
    "clean:cache": "rimraf -g **/*.cache.json **/auto-*.json **/auto-*.d.ts",
    "clean:lib": "rimraf -g **/node_modules",
    "clean:output": "rimraf dist",
    "lib:up": "taze -I -r --exclude pnpm"
  },
  "dependencies": {
    "@types/react": "^19.2.7",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@pawover/eslint-rules": "0.0.0-alpha.8",
    "@stylistic/eslint-plugin": "^5.6.1",
    "eslint": "^9.39.1",
    "eslint-plugin-antfu": "^3.1.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "taze": "^19.9.2",
    "tsdown": "^0.17.3",
    "type-fest": "^5.3.1",
    "typescript-eslint": "^8.49.0"
  },
  "peerDependencies": {
    "@types/react": ">=19.2.4",
    "typescript": ">=5.9.3"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  }
}
